FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0.1";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];
oriented        oriented;


internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    lower
    {
        type            calculated;
        value           uniform 0;
    }
    upper
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    empty
    {
        type            empty;
        value           nonuniform 0();
    }
    overset
    {
        type            calculated;
        value           uniform 0;
    }
    cylinder
    {
        type            calculated;
        value           uniform 0;
    }
    beam
    {
        type            calculated;
        value           uniform 0;
    }
}


