writeInterval   0.001;

//writeControl    timeStep;

//writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           10.0;

maxDeltaT       0.1;

